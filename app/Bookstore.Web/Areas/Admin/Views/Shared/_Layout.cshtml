<!DOCTYPE html>
<html lang="en">

<head>
    <script>
        var base = document.createElement('base');
        base.href = window.location.protocol + '//' + window.location.hostname + "/proxy/5001/";
        var head = document.getElementsByTagName('head')[0];
        head.insertBefore(base, head.firstChild);
    </script>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewData["Title"] - Bob's Used Books</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
</head>

<body>
    <partial name="_NavBarPartial" />

    <div class="container-fluid pb-5 mb-5">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted " style="bottom:0;">
        <div class="container-fluid">
            &copy; <span id="year">@DateTime.Now.Year</span> - Bob's Used Books - <a asp-area="" asp-controller="Home"
                asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="lib/jquery/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3"
        crossorigin="anonymous"></script>
    @RenderSection("Scripts", false)
</body>

</html>
